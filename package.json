{
    "scripts": {
      "clean": "rm -rf node_modules yarn-error.log css static/css *.log *.sqlite build/ .extra",
      "watch": "watch -p './**/*.md' -c 'yarn run build'",
      "build": "documentation-builder --base-directory . --output-path build --output-media-path 'build/media' --media-url '/media' --tag-manager-code 'GTM-K92JCQ' --search-domain 'docs.conjure-up.io' --search-url 'https://docs.ubuntu.com/search' --search-placeholder 'Search conjure-up docs' --no-link-extensions --build-version-branches --site-root https://conjure-up.io/"
    },
    "dependencies": {
      "watch-cli": "^0.2.2"
  }
}
